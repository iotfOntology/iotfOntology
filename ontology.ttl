@prefix : <http://www.semanticweb.org/orangeinnovation/IoTF#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/orangeinnovation/IoTF#> .

<http://www.semanticweb.org/orangeinnovation/IoTF> rdf:type owl:Ontology ;
                                                    owl:imports <https://saref.etsi.org/core/> ;
                                                    <http://purl.org/dc/terms/description> "IoT-F reprensents failures of IoT systems." ;
                                                    <http://purl.org/dc/terms/title> "IoT Failure ontology" ;
                                                    <http://purl.org/vocab/vann/preferredNamespaceUri> "http://www.semanticweb.org/orangeinnovation/IoTF" ;
                                                    owl:versionIRI "http://www.semanticweb.org/orangeinnovation/IoTF/0.0.1" ;
                                                    owl:versionInfo "0.0.1" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
<http://purl.org/dc/terms/issued> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
<http://purl.org/dc/terms/modified> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
<http://purl.org/dc/terms/source> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/example
<http://purl.org/vocab/vann/example> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/orangeinnovation/IoTF#BatteryLevel
:BatteryLevel rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/orangeinnovation/IoTF#CPUUsage
:CPUUsage rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/orangeinnovation/IoTF#MemoryUsage
:MemoryUsage rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/orangeinnovation/IoTF#NetworkI/O
<http://www.semanticweb.org/orangeinnovation/IoTF#NetworkI/O> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionIRI
owl:versionIRI rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/orangeinnovation/IoTF#becauseOf
:becauseOf rdf:type owl:ObjectProperty ;
           rdfs:domain :FailureMode ;
           rdfs:range :FailureMode .


###  http://www.semanticweb.org/orangeinnovation/IoTF#effectIs
:effectIs rdf:type owl:ObjectProperty ;
          rdfs:domain :FailureMode ;
          rdfs:range :FailureMode .


###  http://www.semanticweb.org/orangeinnovation/IoTF#happensAt
:happensAt rdf:type owl:ObjectProperty ;
           rdfs:domain :FailureMode ;
           rdfs:range :DeviceType .


###  http://www.semanticweb.org/orangeinnovation/IoTF#hasDeviceType
:hasDeviceType rdf:type owl:ObjectProperty ;
               rdfs:domain <https://saref.etsi.org/core/Device> ;
               rdfs:range :DeviceType .


###  http://www.semanticweb.org/orangeinnovation/IoTF#hasRecoveryAction
:hasRecoveryAction rdf:type owl:ObjectProperty ;
                   rdfs:domain :FailureMode ;
                   rdfs:range :RecoveryAction .


###  http://www.semanticweb.org/orangeinnovation/IoTF#isCausedBy
:isCausedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :FailureMode ;
            rdfs:range :FailureCause .


###  http://www.semanticweb.org/orangeinnovation/IoTF#producesEffect
:producesEffect rdf:type owl:ObjectProperty ;
                rdfs:domain :FailureMode ;
                rdfs:range :FailureEffect .


###  https://saref.etsi.org/core/accomplishes
<https://saref.etsi.org/core/accomplishes> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf <https://saref.etsi.org/core/isAccomplishedBy> ;
                                           rdfs:range <https://saref.etsi.org/core/Task> ;
                                           rdfs:comment "A relationship between a certain entity (e.g., a device) and the task it accomplishes"@en ;
                                           rdfs:label "accomplishes"@en .


###  https://saref.etsi.org/core/actsUpon
<https://saref.etsi.org/core/actsUpon> rdf:type owl:ObjectProperty ;
                                       rdfs:domain <https://saref.etsi.org/core/Command> ;
                                       rdfs:range <https://saref.etsi.org/core/State> ;
                                       rdfs:comment "A relationship between a command and a state"@en ;
                                       rdfs:label "acts upon"@en .


###  https://saref.etsi.org/core/consistsOf
<https://saref.etsi.org/core/consistsOf> rdf:type owl:ObjectProperty ;
                                         rdfs:comment "A relationship indicating a composite entity that consists of other entities (e.g., a temperature/humidity sensor that consists of a temperature sensor and a humidity sensor)"@en ;
                                         rdfs:label "consists of"@en .


###  https://saref.etsi.org/core/controlsProperty
<https://saref.etsi.org/core/controlsProperty> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://saref.etsi.org/core/Device> ;
                                               rdfs:range <https://saref.etsi.org/core/Property> ;
                                               rdfs:comment "A relationship specifying the property that can be controlled by a certain device"@en ;
                                               rdfs:label "controls property"@en .


###  https://saref.etsi.org/core/hasCommand
<https://saref.etsi.org/core/hasCommand> rdf:type owl:ObjectProperty ;
                                         owl:inverseOf <https://saref.etsi.org/core/isCommandOf> ;
                                         rdfs:domain <https://saref.etsi.org/core/Function> ;
                                         rdfs:range <https://saref.etsi.org/core/Command> ;
                                         rdfs:comment "A relationship between a function and a command"@en ;
                                         rdfs:label "has command"@en .


###  https://saref.etsi.org/core/hasFunction
<https://saref.etsi.org/core/hasFunction> rdf:type owl:ObjectProperty ;
                                          rdfs:domain <https://saref.etsi.org/core/Device> ;
                                          rdfs:range <https://saref.etsi.org/core/Function> ;
                                          rdfs:comment "A relationship identifying the function of a device"@en ;
                                          rdfs:label "has function"@en .


###  https://saref.etsi.org/core/hasMeasurement
<https://saref.etsi.org/core/hasMeasurement> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <https://saref.etsi.org/core/FeatureOfInterest> ;
                                             rdfs:range <https://saref.etsi.org/core/Measurement> ;
                                             rdfs:comment "A relationship between a feature of interest and a measurement about it"@en ;
                                             rdfs:label "has measurement"@en .


###  https://saref.etsi.org/core/hasMeterReading
<https://saref.etsi.org/core/hasMeterReading> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <https://saref.etsi.org/core/MeteringFunction> ;
                                              rdfs:range <https://saref.etsi.org/core/Measurement> ;
                                              rdfs:comment "A relationship between a metering function and the measurement of the reading"@en ;
                                              rdfs:label "has meter reading"@en .


###  https://saref.etsi.org/core/hasMeterReadingType
<https://saref.etsi.org/core/hasMeterReadingType> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain <https://saref.etsi.org/core/MeteringFunction> ;
                                                  rdfs:range <https://saref.etsi.org/core/Property> ;
                                                  rdfs:comment "A relationship identifying the reading type of a metering function (e.g., Water, Gas, Pressure , Energy , Power, etc.)"@en ;
                                                  rdfs:label "has meter reading type"@en .


###  https://saref.etsi.org/core/hasPrice
<https://saref.etsi.org/core/hasPrice> rdf:type owl:ObjectProperty ;
                                       rdfs:range <https://saref.etsi.org/core/Price> ;
                                       rdfs:comment "A relationship indentifying the price associated to an entity"@en ;
                                       rdfs:label "has price"@en .


###  https://saref.etsi.org/core/hasProfile
<https://saref.etsi.org/core/hasProfile> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <https://saref.etsi.org/core/Device> ;
                                         rdfs:range <https://saref.etsi.org/core/Profile> ;
                                         rdfs:comment "A relationship associating a profile to a certain device"@en ;
                                         rdfs:label "has profile"@en .


###  https://saref.etsi.org/core/hasProperty
<https://saref.etsi.org/core/hasProperty> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <https://saref.etsi.org/core/isPropertyOf> ;
                                          rdfs:domain <https://saref.etsi.org/core/FeatureOfInterest> ;
                                          rdfs:range <https://saref.etsi.org/core/Property> ;
                                          rdfs:comment "A relationship between a feature of interest and a property of that feature"@en ;
                                          rdfs:label "has property"@en .


###  https://saref.etsi.org/core/hasSensingRange
<https://saref.etsi.org/core/hasSensingRange> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <https://saref.etsi.org/core/SensingFunction> ;
                                              rdfs:range <https://saref.etsi.org/core/Measurement> ;
                                              rdfs:comment "A relationship between a sensing function and a measurement identifying the range of a sensor detection"@en ;
                                              rdfs:label "has sensing range"@en .


###  https://saref.etsi.org/core/hasSensorType
<https://saref.etsi.org/core/hasSensorType> rdf:type owl:ObjectProperty ;
                                            rdfs:domain <https://saref.etsi.org/core/SensingFunction> ;
                                            rdfs:range <https://saref.etsi.org/core/Property> ;
                                            rdfs:comment "A relationship identifying the sensing type of a sensor detection (i.e., Temperature, Occupancy, Humidity, Motion , Smoke, Pressure, etc.) "@en ;
                                            rdfs:label "has sensor type"@en .


###  https://saref.etsi.org/core/hasState
<https://saref.etsi.org/core/hasState> rdf:type owl:ObjectProperty ;
                                       rdfs:domain <https://saref.etsi.org/core/Device> ;
                                       rdfs:range <https://saref.etsi.org/core/State> ;
                                       rdfs:comment "A relationship identifying the type of state of a device"@en ;
                                       rdfs:label "has state"@en .


###  https://saref.etsi.org/core/hasThresholdMeasurement
<https://saref.etsi.org/core/hasThresholdMeasurement> rdf:type owl:ObjectProperty ;
                                                      rdfs:domain <https://saref.etsi.org/core/EventFunction> ;
                                                      rdfs:range <https://saref.etsi.org/core/Measurement> ;
                                                      rdfs:comment "A relationship associated with an event function to notify that a certain threshold measurement has been exceeded"@en ;
                                                      rdfs:label "has threshold measurement"@en .


###  https://saref.etsi.org/core/hasTime
<https://saref.etsi.org/core/hasTime> rdf:type owl:ObjectProperty ;
                                      rdfs:range <https://saref.etsi.org/core/Time> ;
                                      rdfs:comment "A relationship to associate time information to an entity"@en ;
                                      rdfs:label "has time"@en .


###  https://saref.etsi.org/core/hasTypicalConsumption
<https://saref.etsi.org/core/hasTypicalConsumption> rdf:type owl:ObjectProperty ;
                                                    rdfs:comment "A relationship identifying the typical (energy or power) consumption of a device"@en ;
                                                    rdfs:label "has typical consumption"@en .


###  https://saref.etsi.org/core/isAbout
<https://saref.etsi.org/core/isAbout> rdf:type owl:ObjectProperty ;
                                      rdfs:comment "A relationship identifying what an entity, such as a profile, is about"@en ;
                                      rdfs:label "isAbout"@en .


###  https://saref.etsi.org/core/isAccomplishedBy
<https://saref.etsi.org/core/isAccomplishedBy> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://saref.etsi.org/core/Task> ;
                                               rdfs:comment "A relationship identifying an entity (e.g., a device) that can accomplish a task."@en ;
                                               rdfs:label "is accomplished by"@en .


###  https://saref.etsi.org/core/isCommandOf
<https://saref.etsi.org/core/isCommandOf> rdf:type owl:ObjectProperty ;
                                          rdfs:domain <https://saref.etsi.org/core/Command> ;
                                          rdfs:range <https://saref.etsi.org/core/Function> ;
                                          rdfs:comment "A relationship between a command and a function."@en ;
                                          rdfs:label "is command of"@en .


###  https://saref.etsi.org/core/isControlledByDevice
<https://saref.etsi.org/core/isControlledByDevice> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <https://saref.etsi.org/core/Property> ;
                                                   rdfs:range <https://saref.etsi.org/core/Device> ;
                                                   rdfs:comment "A relationship specifying the devices that can control a certain property"@en ;
                                                   rdfs:label "is controlled by device"@en .


###  https://saref.etsi.org/core/isMeasuredByDevice
<https://saref.etsi.org/core/isMeasuredByDevice> rdf:type owl:ObjectProperty ;
                                                 rdfs:domain <https://saref.etsi.org/core/Property> ;
                                                 rdfs:range <https://saref.etsi.org/core/Device> ;
                                                 rdfs:comment "A relationship specifying the devices that can measure a certain property"@en ;
                                                 rdfs:label "is measured by device"@en .


###  https://saref.etsi.org/core/isMeasuredIn
<https://saref.etsi.org/core/isMeasuredIn> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <https://saref.etsi.org/core/Measurement> ;
                                           rdfs:range <https://saref.etsi.org/core/UnitOfMeasure> ;
                                           rdfs:comment "A relationship identifying the unit of measure used for a certain entity."@en ;
                                           rdfs:label "is measured in"@en .


###  https://saref.etsi.org/core/isMeasurementOf
<https://saref.etsi.org/core/isMeasurementOf> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <https://saref.etsi.org/core/hasMeasurement> ;
                                              rdfs:domain <https://saref.etsi.org/core/Measurement> ;
                                              rdfs:range <https://saref.etsi.org/core/FeatureOfInterest> ;
                                              rdfs:comment "A relationship between a measurement and the feature of interest whose quality was measured"@en ;
                                              rdfs:label "isMeasurementOf"@en .


###  https://saref.etsi.org/core/isOfferedBy
<https://saref.etsi.org/core/isOfferedBy> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <https://saref.etsi.org/core/offers> ;
                                          rdfs:domain <https://saref.etsi.org/core/Service> ;
                                          rdfs:range <https://saref.etsi.org/core/Device> ;
                                          rdfs:comment "A relationship between a service and a device that offers the service"@en ;
                                          rdfs:label "is offered by"@en .


###  https://saref.etsi.org/core/isPropertyOf
<https://saref.etsi.org/core/isPropertyOf> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <https://saref.etsi.org/core/Property> ;
                                           rdfs:range <https://saref.etsi.org/core/FeatureOfInterest> ;
                                           rdfs:comment "A relationship between a property and the feature of interest it belongs to"@en ;
                                           rdfs:label "is property of"@en .


###  https://saref.etsi.org/core/isUsedFor
<https://saref.etsi.org/core/isUsedFor> rdf:type owl:ObjectProperty ;
                                        rdfs:comment "A relationship specifying the commodity for which a device is used for"@en ;
                                        rdfs:label "is used for"@en .


###  https://saref.etsi.org/core/makesMeasurement
<https://saref.etsi.org/core/makesMeasurement> rdf:type owl:ObjectProperty ;
                                               owl:inverseOf <https://saref.etsi.org/core/measurementMadeBy> ;
                                               rdfs:domain <https://saref.etsi.org/core/Device> ;
                                               rdfs:range <https://saref.etsi.org/core/Measurement> ;
                                               rdfs:comment "A relation between a device and the measurements it makes. Such measurement will link together the value of the measurement, its unit of measure and the property to which it relates."@en ;
                                               rdfs:label "makes measurement"@en .


###  https://saref.etsi.org/core/measurementMadeBy
<https://saref.etsi.org/core/measurementMadeBy> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <https://saref.etsi.org/core/Measurement> ;
                                                rdfs:range <https://saref.etsi.org/core/Device> ;
                                                rdfs:comment "A relation between a measurement and the device that made it."@en ;
                                                rdfs:label "measurement made by"@en .


###  https://saref.etsi.org/core/measuresProperty
<https://saref.etsi.org/core/measuresProperty> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://saref.etsi.org/core/Device> ;
                                               rdfs:range <https://saref.etsi.org/core/Property> ;
                                               rdfs:comment "A relationship specifying the property that can be measured by a certain device"@en ;
                                               rdfs:label "measures property"@en .


###  https://saref.etsi.org/core/offers
<https://saref.etsi.org/core/offers> rdf:type owl:ObjectProperty ;
                                     rdfs:domain <https://saref.etsi.org/core/Device> ;
                                     rdfs:range <https://saref.etsi.org/core/Service> ;
                                     rdfs:comment "A relationship between a device and a service"@en ;
                                     rdfs:label "offers"@en .


###  https://saref.etsi.org/core/relatesToMeasurement
<https://saref.etsi.org/core/relatesToMeasurement> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <https://saref.etsi.org/core/Property> ;
                                                   rdfs:range <https://saref.etsi.org/core/Measurement> ;
                                                   rdfs:comment "A relationship between a property and the measurements it relates to"@en ;
                                                   rdfs:label "relates to measurement"@en .


###  https://saref.etsi.org/core/relatesToProperty
<https://saref.etsi.org/core/relatesToProperty> rdf:type owl:ObjectProperty ;
                                                owl:inverseOf <https://saref.etsi.org/core/relatesToMeasurement> ;
                                                rdfs:domain <https://saref.etsi.org/core/Measurement> ;
                                                rdfs:range <https://saref.etsi.org/core/Property> ;
                                                rdfs:comment "A relationship between a measurement and the property it relates to"@en ;
                                                rdfs:label "relates to property"@en .


###  https://saref.etsi.org/core/represents
<https://saref.etsi.org/core/represents> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <https://saref.etsi.org/core/Service> ;
                                         rdfs:range <https://saref.etsi.org/core/Function> ;
                                         rdfs:comment "A relationship between a service and a function."@en ;
                                         rdfs:label "represents"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/orangeinnovation/IoTF#hasFailureCode
:hasFailureCode rdf:type owl:DatatypeProperty ;
                rdfs:domain :FailureMode ;
                rdfs:range xsd:string .


###  https://saref.etsi.org/core/hasDescription
<https://saref.etsi.org/core/hasDescription> rdf:type owl:DatatypeProperty ;
                                             rdfs:comment "A relationship providing a description of an entity (e.g., device). The value is expected to be a string or a string with language tag."@en ,
                                                          "DEPRECATED: The use of rdfs:comment is recommended instead."@en ;
                                             rdfs:label "has description"@en ;
                                             owl:deprecated "true"^^xsd:boolean .


###  https://saref.etsi.org/core/hasManufacturer
<https://saref.etsi.org/core/hasManufacturer> rdf:type owl:DatatypeProperty ;
                                              rdfs:comment "A relationship identifying the manufacturer of an entity (e.g., device). The value is expected to be a string or a string with language tag."@en ;
                                              rdfs:label "has manufacturer"@en .


###  https://saref.etsi.org/core/hasModel
<https://saref.etsi.org/core/hasModel> rdf:type owl:DatatypeProperty ;
                                       rdfs:comment "A relationship identifying the model of an entity (e.g., device). The value is expected to be a string or a string with language tag."@en ;
                                       rdfs:label "has model"@en .


###  https://saref.etsi.org/core/hasTimestamp
<https://saref.etsi.org/core/hasTimestamp> rdf:type owl:DatatypeProperty ;
                                           rdfs:comment "A relationship stating the timestamp of an entity (e.g. a measurement)."@en ;
                                           rdfs:label "has timestamp"@en .


###  https://saref.etsi.org/core/hasValue
<https://saref.etsi.org/core/hasValue> rdf:type owl:DatatypeProperty ;
                                       rdfs:comment "A relationship defining the value of a certain property, e.g., energy or power. Note that, even if numeric values are expected to enable reasoning, measurement values could use other datatypes."@en ;
                                       rdfs:label "has value"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/orangeinnovation/IoTF#AudioOutputFault
:AudioOutputFault rdf:type owl:Class ;
                  rdfs:subClassOf :HardwareFailure .


###  http://www.semanticweb.org/orangeinnovation/IoTF#BLEFault
:BLEFault rdf:type owl:Class ;
          rdfs:subClassOf :RadioFault .


###  http://www.semanticweb.org/orangeinnovation/IoTF#BatteryFluctuation
:BatteryFluctuation rdf:type owl:Class ;
                    rdfs:subClassOf :PowerFailure .


###  http://www.semanticweb.org/orangeinnovation/IoTF#BatteryLevel
:BatteryLevel rdf:type owl:Class ;
              rdfs:subClassOf :Symptom .


###  http://www.semanticweb.org/orangeinnovation/IoTF#CPUUsage
:CPUUsage rdf:type owl:Class ;
          rdfs:subClassOf :Symptom .


###  http://www.semanticweb.org/orangeinnovation/IoTF#Calibration
:Calibration rdf:type owl:Class ;
             rdfs:subClassOf :NonFailStop ;
             rdfs:comment "happens when sensor data may have offset or have a different gain from the ground truth values." .


###  http://www.semanticweb.org/orangeinnovation/IoTF#CellularFault
:CellularFault rdf:type owl:Class ;
               rdfs:subClassOf :RadioFault .


###  http://www.semanticweb.org/orangeinnovation/IoTF#CommunicationFailure
:CommunicationFailure rdf:type owl:Class ;
                      rdfs:subClassOf :FailureMode .


###  http://www.semanticweb.org/orangeinnovation/IoTF#ConnectivityFailure
:ConnectivityFailure rdf:type owl:Class ;
                     rdfs:subClassOf :FailureCause ;
                     rdfs:comment "represents failure cause related to the connectivity." .


###  http://www.semanticweb.org/orangeinnovation/IoTF#DeviceType
:DeviceType rdf:type owl:Class ;
            rdfs:comment "denotes a type of a market IoT device e.g., FIBARO light bulb." .


###  http://www.semanticweb.org/orangeinnovation/IoTF#Environnment
:Environnment rdf:type owl:Class ;
              rdfs:subClassOf :FailureCause ;
              rdfs:comment "represents failure cause related to the environment." .


###  http://www.semanticweb.org/orangeinnovation/IoTF#EthernetFault
:EthernetFault rdf:type owl:Class ;
               rdfs:subClassOf :RadioFault .


###  http://www.semanticweb.org/orangeinnovation/IoTF#FailStop
:FailStop rdf:type owl:Class ;
          rdfs:subClassOf :FailureMode ;
          rdfs:comment "happens when a device stops functioning and is un-responsive to external requests." .


###  http://www.semanticweb.org/orangeinnovation/IoTF#FailureCause
:FailureCause rdf:type owl:Class ;
              rdfs:comment "denotes the cause of a failure." .


###  http://www.semanticweb.org/orangeinnovation/IoTF#FailureEffect
:FailureEffect rdf:type owl:Class ;
               rdfs:comment "represents the effects of a failure mode." .


###  http://www.semanticweb.org/orangeinnovation/IoTF#FailureMode
:FailureMode rdf:type owl:Class ;
             rdfs:comment "a form of a dysfunction related to an IoTdevice." .


